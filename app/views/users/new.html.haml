#middleleftPan
  %h2 New User
  %p Please enter your information
  = render partial: "shared/print_errors", locals: {obj: @user}


#middlerightPan
  = form_for @user do |f|
    %h2 User data
    .form_row
      = f.label :name, "Nombre"
      = f.email_field :name
    .form_row
      = f.label :email
      = f.email_field :email
    .form_row
      = f.label :password
      = f.password_field :password
    .form_row
      = f.label :password_confirmation
      = f.password_field :password_confirmation
    #register
    = f.submit "UNETE", class: "button"