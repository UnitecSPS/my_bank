= render "submenu"

.center_content
  #right_wrap
    #right_content
      %h2 Clientes

      - if flash[:notice].present?
        .notice= flash[:notice]

      - if @clients.any?
        = render partial: "shared/filter_form", locals: {form_path: clients_path}
        
        .js_filtered_table
          = render partial: "table", locals: {clients: @clients}
      - else
        %em No Hay ningun cliente agredo
  .sidebar{id: "sidebar"}
    %h2 Administracion de usuarios
    .sidebar_section_text Listado de clientes para que puedas agregar nuevos u editar existentes.

  .clear



